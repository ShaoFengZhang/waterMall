#scrollBox {
    width: 100%;
    box-sizing: border-box;
    padding: 0;
    background-repeat: no-repeat;
    background-origin: border-box;
    position: relative;
    overflow: hidden;
    background-image: url('https://tp.datikeji.com/a/15422612654834/T0tndE3B5trhC5UP8hIhXTLicGHFjEeJn7JEPu1Y.png');
    background-size: 750rpx 340rpx;
}

.myqr {
	position: absolute;
    top: 76rpx;
    right: 40rpx;
}

.myqr image {
    vertical-align: middle;
}

#baoBtn {
    width: 100%;
    height: 100%;
    padding: 0;
    position: absolute;
    top: 0;
    left: 0;
}

.topPrompt {
    width: 134px;
    height: 29px;
    position: absolute;
    top: 0;
    right: 22px;
	text-align: center;
	line-height: 34px;
	font-size: 12px;
	color: #E02E24;
}

.topPrompt view{
	width: 134px;
    height: 29px;
	position: absolute;
	top: 0;
	left: 0;
}

.topPrompt text{
	display: inline-block;
	width: 16px;
	height: 29px;
	line-height: 34px;
	text-align: center;
}

.topPrompt image {
    width: 134px;
    height: 29px;
}

.userInfo {
    width: 750rpx;
    height: 158rpx;
    box-sizing: border-box;
    padding: 30rpx 0 0 30rpx;
    display: flex;
    justify-content: space-between;
}

.infoLeft {
    width: 128rpx;
    height: 128rpx;
    border-radius: 50%;
    overflow: hidden;
    border: 4rpx solid white;
    box-sizing: border-box;
}

.infoRight {
    width: 568rpx;
    height: 128rpx;
    color: white;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.userName {
    font-size: 34rpx;
}

.userDoller {
    font-size: 30rpx;
}

.userDoller text {
    font-size: 36rpx;
    font-weight: bold;
}

.userData {
    width: 690rpx;
    height: 128rpx;
    background-color: #fff;
    box-shadow: 0rpx 0rpx 20rpx 0rpx rgba(111, 105, 103, 0.16);
    border-radius: 8rpx;
    margin: 60rpx auto 0;
    box-sizing: border-box;
    display: flex;
    justify-content: space-around;
}

.userDataItem {
    text-align: center;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.orderTxt {
    width: 690rpx;
    margin: 34rpx auto 0;
    color: #333;
    font-size: 30rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 500;
}

.orderItem {
    width: 580rpx;
    height: 146rpx;
    margin: -8rpx auto 0;
    box-sizing: border-box;
    padding: 30rpx 0 28rpx;
    display: flex;
    justify-content: space-between;
}

.orderDataItem {
    color: #333;
    font-size: 26rpx;
    text-align: center;
}

.orderDataItem image {
    width: 54rpx;
    height: 54rpx;
}

.orderDataItem view{
	margin-top: 0rpx;
}

.shareFriend {
    width: 690rpx;
    height: 222rpx;
    margin: 8rpx auto 0;
    border-radius: 8rpx;
    color: #fff;
    text-align: center;
    background: url('https://tp.datikeji.com/a/15441697067354/XLJ1zUO2LUZVVNCnvP60zZa59DLWcC2LqVgRaJqF.png');
    box-sizing: border-box;
    background-size: cover;
    background-repeat: no-repeat;
    background-origin: border-box;
    position: relative;
    overflow: hidden;
}

.shareFriendIcon {
    width: 668rpx;
    height: 125rpx;
    margin: 88rpx auto 0;
    background-image: url('https://tp.datikeji.com/a/15434584676256/oP9uCmLlkviRbmXm2mUbfHWb5icIQCdsLVf0ixML.png');
    background-repeat: no-repeat;
    background-size: 668rpx 125rpx;
    box-sizing: border-box;
    position: relative;
    overflow: hidden;
    display: flex;
    justify-content: conter;
	align-items:center;
}

.shareFriendLeftIcon {
    width: 290rpx;
    height: 100%;
    box-sizing: border-box;
    padding-left: 24rpx;
    text-align: center;
    position: relative;
}

.shareFriendRightIcon {
    width: 290rpx;
    height: 100%;
    box-sizing: border-box;
    padding-right: 24rpx;
    text-align: center;
    position: relative;
}

.leftArrow, .rightArrow {
    width: 14rpx;
    height: 26rpx;
    position: absolute;
    top: 44rpx;
}

.leftArrow {
    left: 208rpx;
}

.rightArrow {
    left: 190rpx;
}

.invitationIcon, .invitationIcon image {
    width: 110rpx;
    height: 106rpx;
}

.promoteBox {
    width: 690rpx;
    height: 172rpx;
    margin: 0 auto;
    border-bottom: 2rpx solid #eaeaea;
    display: flex;
    justify-content: center;
    box-sizing: border-box;
    padding: 0 16rpx;
}

.promoteItem {
    color: #333;
    font-size: 26rpx;
    text-align: center;
    margin-top: 30rpx;
}

.promoteItem:first-child {
    margin-right: 222rpx;
}

.promoteItem image {
    width: 78rpx;
    height: 78rpx;
}

.saveMoneyBox {
    width: 690rpx;
    height: 102rpx;
    line-height: 102rpx;
    margin: 0rpx auto 0;
}

.botLine {
    width: 624rpx;
    height: 2rpx;
    background: rgba(234, 234, 234, 1);
    margin-left: 96rpx;
	margin-top: 0rpx;
}

.haveProblem {
    width: 690rpx;
    height: 102rpx;
    margin: 0 auto;
    line-height: 102rpx;
}

.botlist {
    color: #333;
    font-size: 32rpx;
    /* font-family: PingFang-SC-Medium;
    font-weight: 500; */
}

.botlist image {
    width: 54rpx;
    height: 54rpx;
    vertical-align: middle;
}

/* 弹窗 */

.serviceMask {
    width: 100%;
    height: 100vh;
    background-color: rgba(51, 51, 51, 0.5);
    position: absolute;
    top: 0;
    left: 0;
}

.MaskBody {
    width: 530rpx;
    height: 600rpx;
    background: rgba(254, 252, 253, 1);
    border-radius: 32rpx;
    margin: 268rpx auto 0;
    overflow: hidden;
    position: relative;
}

.maskPromtTxt {
    font-size: 40rpx;
    font-weight: bold;
    color: rgba(51, 51, 51, 1);
    line-height: 60rpx;
    text-align: center;
    margin-top: 80rpx;
    margin-bottom: 68rpx;
}

.MaskBody image {
    width: 38rpx;
    height: 56rpx;
    position: absolute;
    top: 50rpx;
    left: 0;
}

.txtDetil {
    text-align: center;
    font-size: 34rpx;
    line-height: 72rpx;
}

#customerBtn {
    width: 360rpx;
    height: 68rpx;
    background: rgba(224, 46, 36, 1);
    border-radius: 34rpx;
    margin: 60rpx auto 0;
    font-size: 30rpx;
    color: rgba(255, 255, 255, 1);
    line-height: 68rpx;
}

