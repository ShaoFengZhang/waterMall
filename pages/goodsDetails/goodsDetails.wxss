/* 步骤效果 */

.StepItem1 {
    animation: StepItem1 5s ease-in-out infinite;
}
.StepItem2 {
    animation: StepItem2 5s ease-in-out infinite;
}
.StepItem3 {
    animation: StepItem3 5s ease-in-out infinite;
}
.StepItem4 {
    animation: StepItem4 5s ease-in-out infinite;
}

@keyframes StepItem1 {
	0% {
        opacity: 0.3;
    }

    25% {
        opacity: 1;
    }

    50% {
         opacity: 0.3;
    }

    75% {
         opacity: 0.3;
    }

	100% {
         opacity: 0.3;
    }
}

@keyframes StepItem2 {
	0% {
         opacity: 0.3;
    }

    25% {
         opacity: 0.3;
    }

    50% {
        opacity: 1;
    }

    75% {
         opacity: 0.3;
    }

	100% {
        opacity: 0.3;
    }
}

@keyframes StepItem3 {
	0% {
         opacity: 0.3;
    }

    25% {
         opacity: 0.3;
    }

    50% {
         opacity: 0.3;
    }

    75% {
        opacity: 1;
    }

	100% {
         opacity: 0.3;
    }
}

@keyframes StepItem4 {
	0% {
         opacity: 0.3;
    }

    25% {
         opacity: 0.3;
    }

    50% {
        opacity: 0.3;
    }

    75% {
         opacity: 0.3;
    }

	100% {
        opacity: 1;
    }
}
