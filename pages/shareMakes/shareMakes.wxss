/* 能量动效 */

.ani1 {
    animation: ani1 4s linear 0.5s infinite;
}

@keyframes ani1 {
    0% {
        transform: translateY(0px);
    }

    25% {
        transform: translateY(5px);
    }

    50% {
        transform: translateY(0px);
    }

    75% {
        transform: translateY(-5px);
    }

    100% {
        transform: translateY(0px);
    }
}

.ani2 {
    animation: ani2 4s linear 0.5s infinite;
}

@keyframes ani2 {
    0% {
        transform: translateY(0px);
    }

    25% {
        transform: translateY(-5px);
    }

    50% {
        transform: translateY(0px);
    }

    75% {
        transform: translateY(5px);
    }

    100% {
        transform: translateY(0px);
    }
}

.nAni1 {
    animation: nAni1 1.2s linear 0.5s 1;
}

@keyframes nAni1 {
    0% {
        transform: translate(0px, 0px) scale(1);
        opacity: 1;
    }

    100% {
        transform: translate(120px, 50px) scale(0);
        opacity: 0;
    }
}

.nAni2 {
    animation: nAni2 1.2s linear 0.5s 1;
}

@keyframes nAni2 {
    0% {
        transform: translate(-0px, -0px) scale(1);
        opacity: 1;
    }

    100% {
        transform: translate(-160px, -60px) scale(0);
        opacity: 0;
    }
}

.nAni3 {
    animation: nAni3 1.2s linear 0.5s 1;
}

@keyframes nAni3 {
    0% {
        transform: translate(0px, 0px) scale(1);
        opacity: 1;
    }

    100% {
        transform: translate(120px, -50px) scale(0);
        opacity: 0;
    }
}

.CnAni3 {
    animation: CnAni3 1.2s linear 0.5s 1;
}

@keyframes CnAni3 {
    0% {
        transform: translate(0px, 0px) scale(1);
        opacity: 1;
    }

    100% {
        transform: translate(0px, 100px) scale(0);
        opacity: 0;
    }
}
