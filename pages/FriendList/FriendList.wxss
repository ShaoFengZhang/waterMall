.parentInfo {
    width: 100%;
    height: 120rpx;
    box-sizing: border-box;
    padding: 0 30rpx;
    margin: 30rpx 0 0;
    display: flex;
    justify-content: flex-start;
}

.parentInfo image {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    box-sizing: border-box;
    border: 4rpx solid #fff;
    margin-right: 20rpx;
    box-shadow: 0px 3rpx 20rpx 0px rgba(121, 121, 121, 0.4);
}

.parentName {
    color: #333;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    box-sizing: border-box;
    padding: 20rpx 0;
}

.parentName view {
    width: 540rpx;
    height: 50rpx;
    line-height: 50rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.friendNumber {
    width: 690rpx;
    height: 196rpx;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px 3rpx 20rpx 0px rgba(111, 105, 103, 0.16);
    border-radius: 8rpx;
    margin: 30rpx auto;
    box-sizing: border-box;
    padding-left: 50rpx;
    font-size: 26rpx;
    color: #333;
    position: relative;
}

.haoyoushu text {
    margin-left: 10rpx;
}

.leijishu text {
    margin-left: 10rpx;
}

.haoyoushu, .leijishu {
    width: 100%;
    height: 97rpx;
    line-height: 97rpx;
}

.haoyoushu2, .leijishu2 {
    width: 100%;
    height: 84rpx;
    line-height: 84rpx;
}

.line {
    width: 368rpx;
    height: 2rpx;
    background: rgba(234, 234, 234, 1);
}

.longLine {
    width: 630rpx;
    height: 1rpx;
    background: rgba(234, 234, 234, 1);
}

#shareID {
    width: 190rpx;
    height: 64rpx;
    background: rgba(224, 46, 36, 1);
    border-radius: 32rpx;
    padding: 0;
    line-height: 64rpx;
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    position: absolute;
    top: 66rpx;
    right: 30rpx;
}

#friendShareID {
    width: 160rpx;
    height: 54rpx;
    border: 2rpx solid rgba(224, 46, 36, 1);
    border-radius: 27rpx;
    padding: 0;
    line-height: 54rpx;
    color: #e02e24;
    font-size: 24rpx;
    text-align: center;
    position: absolute;
    bottom: 58rpx;
    right: 30rpx;
}

.haoyougongxian {
    width: 100%;
    color: #333;
    font-size: 30rpx;
    box-sizing: border-box;
    padding-left: 30rpx;
}

.friendItem {
    width: 100%;
    box-sizing: border-box;
    padding: 0 58rpx 0 62rpx;
    /* margin-bottom: 20rpx; */
    background-color: #fff;
}

.segment{
	width: 750rpx;
	height: 20rpx;
	background-color: #f2f2f2;
	margin-left: -62rpx;
}

.friendItemInfo {
    width: 100%;
    height: 145rpx;
    box-sizing: border-box;
    padding: 32rpx 0 24rpx;
    display: flex;
    justify-content: flex-start;
}

.friendItemInfo image {
    width: 86rpx;
    height: 86rpx;
    box-sizing: border-box;
    border: 2rpx solid #fff;
    box-shadow: 0px 2rpx 14rpx 0px rgba(121, 121, 121, 0.4);
    border-radius: 50%;
    margin-right: 20rpx;
}

.friendItemInfo view:last-child {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.friendName {
    width: 510rpx;
    height: 40rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.botLineTxt {
    width: 100%;
    height: 82rpx;
    line-height: 82rpx;
    text-align: center;
    font-size: 22rpx;
    color: #797979;
    background-color: #fff;
}

.noData {
    width: 466rpx;
    height: 400rpx;
    margin: 104rpx auto 0;
}

.noData image {
    width: 466rpx;
    height: 400rpx;
}

.nodataTxt{
	text-align: center;
	font-size: 26rpx;
	color: #333333;
	margin-top: -60rpx;
}
